{
  "$schema" : "https:\/\/schema.linearmouse.app\/0.9.1",
  "schemes" : [
    {
      "buttons" : {
        "clickDebouncing" : {
          "timeout" : 0
        },
        "universalBackForward" : false
      },
      "if" : {
        "device" : {
          "productName" : "Apple Internal Keyboard \/ Trackpad",
          "productID" : "0x341",
          "category" : "trackpad",
          "vendorID" : "0x5ac"
        }
      },
      "pointer" : {
        "acceleration" : 6.9866,
        "speed" : 0.0867,
        "disableAcceleration" : false
      },
      "scrolling" : {
        "speed" : {
          "vertical" : 5
        },
        "acceleration" : {
          "vertical" : 2
        },
        "modifiers" : {
          "vertical" : {
            "option" : {
              "type" : "none"
            },
            "shift" : {
              "type" : "alterOrientation"
            },
            "control" : {
              "type" : "changeSpeed",
              "scale" : 3
            },
            "command" : {
              "type" : "none"
            }
          }
        },
        "reverse" : {
          "vertical" : false
        },
        "distance" : {
          "vertical" : "auto"
        }
      }
    }
  ]
}